{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-unused-vars": 0,
        "no-case-declarations": 0
    }, "overrides": [
        {
          "files": ["*.test.js"],
          "rules": {
            "no-undef": "off"
          }
        }
      ]
}
